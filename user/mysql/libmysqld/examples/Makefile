link_sources:
	for f in $(client_sources); do \
	  rm -f $(srcdir)/$$f; \
	  /bin/ln -s $(srcdir)/../../client/$$f $(srcdir)/$$f; \
	done;

clean:
	rm -f $(client_sources)

