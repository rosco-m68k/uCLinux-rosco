.sh:
	/bin/rm -f $@ $@-t
	/bin/sed \
	  -e 's!@''bindir''@!$(bindir)!g' \
	  -e 's!@''scriptdir''@!$(bindir)!g' \
	  -e 's!@''prefix''@!$(prefix)!g' \
	  -e 's!@''datadir''@!$(datadir)!g' \
	  -e 's!@''localstatedir''@!$(localstatedir)!g' \
	  -e 's!@''libexecdir''@!$(libexecdir)!g' \
	  -e 's!@''CC''@!gcc!'\
	  -e 's!@''CXX''@!m68k-elf-g++ -m68000!'\
	  -e 's!@''GXX''@!yes!'\
	  -e 's!@''PERL''@!/usr/bin/perl!' \
	  -e 's!@''CFLAGS''@!-DHAVE_GETHOSTBYNAME_R_GLIBC2_STYLE=1!'\
	  -e 's!@''CXXFLAGS''@!-Os -g -fomit-frame-pointer -m68000  -Dlinux -D__linux__ -Dunix -D__uClinux__ -DEMBED -I/home/gerg/new-wave.dist/lib/libc/include -I/home/gerg/new-wave.dist/lib/libm -I/home/gerg/new-wave.dist/lib/libcrypt_old -I/home/gerg/new-wave.dist -fno-builtin -msep-data -I/home/gerg/new-wave.dist/linux-2.0.x/include  -fno-exceptions!'\
	  -e 's!@''LDFLAGS''@!!'\
	  -e 's!@''VERSION''@!4.0.16!' \
	  -e 's!@''MYSQL_SERVER_SUFFIX''@!!' \
	  -e 's!@''COMPILATION_COMMENT''@!Source distribution!' \
	  -e 's!@''MACHINE_TYPE''@!i386!' \
	  -e 's!@''HOSTNAME''@!/bin/hostname!' \
	  -e 's!@''SYSTEM_TYPE''@!redhat-linux!' \
	  -e 's!@''CHECK_PID''@!/bin/kill -0 $$PID > /dev/null 2> /dev/null!' \
	  -e 's!@''FIND_PROC''@!/bin/ps p $$PID | grep mysqld > /dev/null!' \
	  -e 's!@''MYSQLD_DEFAULT_SWITCHES''@!!' \
	  -e 's!@''MYSQL_UNIX_ADDR''@!/tmp/mysql.sock!' \
	  -e 's!@''IS_LINUX''@!false!' \
	  -e "s!@""CONF_COMMAND""@!./configure '-C' '--prefix=' '--build=i386-redhat-linux' '--enable-dependency-tracking' '--target=m68k-elf' '--without-pthread' '--without-readline' '--disable-shared' '--with-headers=/home/gerg/new-wave.dist/linux-2.0.x/include' '--with-mysql=/home/gerg/new-wave.dist/lib/libmysqlclient' '--with-named-z-libs==/home/gerg/new-wave.dist/lib/zlib' 'CFLAGS=-DHAVE_GETHOSTBYNAME_R_GLIBC2_STYLE=1' 'CXXFLAGS=-Os -g -fomit-frame-pointer -m68000  -Dlinux -D__linux__ -Dunix -D__uClinux__ -DEMBED -I/home/gerg/new-wave.dist/lib/libc/include -I/home/gerg/new-wave.dist/lib/libm -I/home/gerg/new-wave.dist/lib/libcrypt_old -I/home/gerg/new-wave.dist -fno-builtin -msep-data -I/home/gerg/new-wave.dist/linux-2.0.x/include  -fno-exceptions' 'CXX=m68k-elf-g++ -m68000' 'build_alias=i386-redhat-linux' 'target_alias=m68k-elf'!" \
	  -e 's!@''MYSQLD_USER''@!mysql!' \
	  -e 's!@''sysconfdir''@!${prefix}/etc!' \
	  -e 's!@''SHORT_MYSQL_INTRO''@!@SHORT_MYSQL_INTRO@!' \
	  -e 's!@''SHARED_LIB_VERSION''@!12:0:0!' \
	  -e 's!@''MYSQL_BASE_VERSION''@!4.0!' \
	  -e 's!@''MYSQL_NO_DASH_VERSION''@!4.0.16!' \
	  -e 's!@''MYSQL_TCP_PORT''@!3306!' \
	  -e 's!@''PERL_DBI_VERSION''@!@PERL_DBI_VERSION@!' \
	  -e 's!@''PERL_DBD_VERSION''@!@PERL_DBD_VERSION@!' \
	  -e 's!@''PERL_DATA_DUMPER''@!@PERL_DATA_DUMPER@!' \
	$< > $@-t
	/bin/chmod +x $@-t
	/bin/mv $@-t $@

