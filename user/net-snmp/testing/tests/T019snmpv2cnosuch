#!/bin/sh

. ../eval_tools.sh

HEADER SNMPv2c get of system.sysUpTime.1

#
# Begin test
#

# standard V2C configuration: testcomunnity
. ./Sv2cconfig

STARTAGENT

CAPTURE "snmpget -On $SNMP_FLAGS -c testcommunity -v 2c udp:localhost:$SNMP_SNMPD_PORT .1.3.6.1.2.1.1.3.1"

STOPAGENT

CHECK ".1.3.6.1.2.1.1.3.1 = No Such Instance"

FINISHED
